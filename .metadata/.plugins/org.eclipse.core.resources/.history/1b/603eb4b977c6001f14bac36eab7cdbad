package day3;

public class Strings {

	public static void main(String[] args) {

		//String is a class so we are creating object
		//Strings are immutable(we cant change the value)
		
		String sname=new String();
		sname="Hinata";
		
		
		String name="Sriahri";
//		sname.equals(name);
		System.out.println(sname.equals(name));
		System.out.println(name.charAt(4));
		System.out.println(name+sname);
		System.out.println(name.endsWith("a"));
		
		//both s1,s2 are locates to the same memory address in stack heap memory
		String s1="Srihari";
		String s2="Srihari";
		
		
	}

}
